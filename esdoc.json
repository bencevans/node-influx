{
  "source": "./lib/src",
  "destination": "./doc",
  "test": {
    "type": "mocha",
    "source": "./lib/test/unit",
    "includes": ["\\.test\\.js$"]
  },
  "plugins": [
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [{"from": "^.+", "to": ""}]
      }
    }
  ],
  "undocumentIdentifier": false,
  "title": "node-influx",
  "manual": {
    "tutorial": [
      "./examples/express_response_times/readme.md"
    ],
    "changelog": ["./CHANGELOG.md"]
  }
}
